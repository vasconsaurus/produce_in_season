<h1 class="page__title"> <%= I18n.t("item_season.index.title") %> </h1>

<table class="produce-table">
  <% ItemSeason::MONTHS.each do |month_index| %>
    <tr>
      <td class="produce-table__month"><%= link_to (formatted_month_name(month_index)).capitalize, item_season_path(month_index), class: "link_action" %></td>
    </tr>
  <%  end %>
</table>
